set nocompatible            " required
filetype off                " required

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

" Vundle is always first
Plugin 'VundleVim/Vundle.vim'

" Other plugins
Plugin 'tpope/vim-fugitive'

" End of Vundle plugins
call vundle#end()
filetype plugin indent on


